{
  "extends": ["eslint:recommended", "plugin:compat/recommended", "prettier"],
  "rules": {
    "no-console": 1
  },
  "plugins": ["compat", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
